{
    "apiConfig": {
        "baseUrl": "http://localhost",
        "ports": {
            "auth_logic": 8080,
            "auth_data": 8081,
            "huertos_logic": 8120,
            "huetos_data": 8121
        },
        "endpoints": {
            "auth": {
                "login": "/api/huertos/v1/login",
                "validateToken": "/auth/v1/validate-token",
                "changePassword": "/api/huertos/v1/change-password"
            },
            "users": {
                "all": "/api/huertos/v1/users",
                "byId": "/api/huertos/v1/users/:user_id",
                "byMemberNumber": "/api/huertos/v1/users/member/:member_number",
                "byPlotNumber": "/api/huertos/v1/users/plot/:plot_number",
                "byDni": "/api/huertos/v1/users/dni/:dni",
                "payments": "/api/huertos/v1/users/:member_number/incomes",
                "hasPaid": "/api/huertos/v1/users/:member_number/haspaid",
                "waitlist": "/api/huertos/v1/users/waitlist",
                "lastMemberNumber": "/api/huertos/v1/users/member/last"
            },
            "incomes": {
                "all": "/api/huertos/v1/incomes",
                "byId": "/api/huertos/v1/incomes/:income_id"
            },
            "expenses": {
                "all": "/api/huertos/v1/expenses",
                "byId": "/api/huertos/v1/expenses/:expense_id"
            },
            "balance": {
                "all": "/api/huertos/v1/balance"
            },
            "announces": {
                "all": "/api/huertos/v1/announces",
                "byId": "/api/huertos/v1/announces/:announce_id"
            },
            "requests": {
                "all": "/api/huertos/v1/requests",
                "byId": "/api/huertos/v1/requests/:request_id"
            },
            "pre_users": {
                "all": "/api/huertos/v1/pre_users",
                "byId": "/api/huertos/v1/pre_users/:pre_user_id"
            }
        }
    }
}